<% if (clients && clients.length > 0) { %>
      
                <% clients.forEach((client) => { %>
               
                        <a href="<%= client.link %>">
                          <img src="<%= process.env.S3_BASE_URL + client.logo %>" alt="<%= client.name %>">
                        </a>
                      
                  <% }) %>
          
      <% } %>