<body>
<div class="page-wrapper">


  <!-- Main Header-->
	<header class="main-header header-style-one">
    <div class="auto-container" style="max-width: none;">
      <!-- Main box -->
      <div class="main-box">
        <div class="logo-box">
          <div class="logo"><a href="/"><img src="/assets/images/Omaya_Logo.svg" alt="Omaya Suites Logo white" ></a></div>
        </div>

        <style>
          .main-box {
            padding: 0 80px 20px 80px;
          }

          @media (max-width: 640px) {
            .main-box {
              padding: 0 20px 20px 20px;
          }
          .logo a img{
            max-height: 40px;
          }
          }
        </style>
        <!--Nav Box-->
        <div class="nav-outer">
          <nav class="nav main-menu">
            <ul class="navigation">
              <li><a href="/">Home</a></li>
              <li><a href="/about">About</a></li>
              <li class="dropdown"><a href="/stay">Stays</a>
                <ul>
                  <li><a href="/stay/royal-suite">ROYAL SUITES</a></li>
                  <li><a href="/stay/deluxe-suite">DELUXE SUITES</a></li>
                  <li><a href="/stay/executive-suite">EXECUTIVE SUITES</a></li>
                  <li><a href="/stay/duo-suite">DUO SUITE</a></li>
                </ul>
              </li>
              <li class="dropdown"><a href="/dining">Dining</a>
                <ul>
                  <li><a href="/dining/pdr">PRIVATE DINING (PDR)</a></li>
                  <li><a href="/dining/oma-kitchen">OMA KITCHEN</a></li>
                  <li><a href="/dining/oma-air-bar">OMA AIR BAR</a></li>
                  <li><a href="/dining/oma-pool-bar">OMA POOL BAR</a></li>
                </ul>
              </li>
              <li class="dropdown"><a href="/venue">Venue</a>
                <ul>
                  <li><a href="/venue/ivory-hall">IVORY HALL</a></li>
                  <li><a href="/venue/silver-hall">SILVER HALL</a></li>
                  <li><a href="/venue/omaya-meet">OMAYA MEET</a></li>
                  <li><a href="/venue/zenith-one-two">ZENITH ONE & TWO</a></li>
                </ul>
              </li>
             
              <li><a href="/gallery">Gallery</a></li>
              <li><a href="/contact">Contact</a></li>
            </ul>
          </nav>
          <!-- Main Menu End-->
        </div>

        <div class="outer-box">
          <% if (typeof itemType !== 'undefined' && itemType === 'stay') { %>
            <button class="header-btn" onclick="openStayBookingModal(); return false;" style=" border: none; cursor: pointer;">Book Now</button>
          <% } else { %>
            <a href="/contact" class="header-btn">Book Now</a>
          <% } %>
          <div class="mobile-nav-toggler"><span class="icon lnr-icon-bars"></span></div>
          <!-- Mobile Nav toggler -->
        </div>
      </div>
      <!-- Mobile Menu  -->
      <div class="mobile-menu">
        <div class="menu-backdrop"></div>
        <!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header-->
        <nav class="menu-box">
          <div class="upper-box">
            <div class="nav-logo"><a href="/"><img src="/assets/images/Omaya_Logo.svg" alt="Omaya Suites Logo"></a></div>
            <div class="close-btn"><i class="icon fa fa-times"></i></div>
          </div>
          <ul class="navigation clearfix">
            <!--Keep This Empty / Menu will come through Javascript-->
          </ul>
          <ul class="contact-list-one">
            <li>
              <!-- Contact Info Box -->
              <div class="contact-info-box">
                <i class="icon lnr-icon-phone-handset"></i>
                <span class="title">Call Now</span>
                <a href="tel:+92880098670">+92 (8800) - 98670</a>
              </div>
            </li>
            <li>
              <!-- Contact Info Box -->
              <div class="contact-info-box">
                <span class="icon lnr-icon-envelope1"></span>
                <span class="title">Send Email</span>
                <a href="https://html.kodesolution.com/cdn-cgi/l/email-protection#5f373a332f1f3c30322f3e3126713c3032"><span class="__cf_email__" data-cfemail="fc9499908cbc9f93918c9d9285d29f9391">[email&#160;protected]</span></a>
              </div>
            </li>
            <li>
              <!-- Contact Info Box -->
              <div class="contact-info-box">
                <span class="icon lnr-icon-clock"></span>
                <span class="title">Send Email</span>
                Mon - Sat 8:00 - 6:30, Sunday - CLOSED
              </div>
            </li>
          </ul>
          <ul class="social-links">
            <li><a href="#"><i class="fa fa-x"></i></a></li>
            <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
            <li><a href="#"><i class="fab fa-pinterest"></i></a></li>
            <li><a href="#"><i class="fab fa-instagram"></i></a></li>
          </ul>
        </nav>
      </div><!-- End Mobile Menu -->

   <style>
    .dropdown ul li a{
      font-family: "Barlow Condensed", sans-serif;
    }
    
    /* Book Now button font - Barlow */
    .header-style-one .outer-box .header-btn {
      font-family: "Barlow", sans-serif !important;
    }
    
    /* Make button look like anchor for Book Now */
    .header-style-one .outer-box .header-btn[onclick] {
      text-decoration: none;
      display: inline-block;
    }
    
    /* Sticky Header Book Now Button */
    .sticky-header-book-btn {
      margin-left: auto;
      display: flex;
      align-items: center;
      padding-right: 20px;
    }
    
    .sticky-header-book-btn .theme-btn {
      padding: 12px 30px;
      font-size: 14px;
      white-space: nowrap;
      font-family: "Barlow", sans-serif;
    }
    
    .sticky-header .inner-container {
      justify-content: space-between;
      align-items: center;
    }
    
    @media (max-width: 991px) {
      .sticky-header-book-btn {
        display: none;
      }
    }
    
    @media (max-width: 1200px) {
      .sticky-header-book-btn {
        padding-right: 15px;
      }
      
      .sticky-header-book-btn .theme-btn {
        padding: 10px 20px;
        font-size: 13px;
      }
    }
   </style>
      <!-- End Header Search -->

      <!-- Sticky Header  -->
      <div class="sticky-header">
        <div class="auto-container">
          <div class="inner-container">
            <!--Logo-->
            <div class="logo">
              <a href="/"><img src="/assets/images/Omaya_Logo_Black.svg" alt="Omaya Suites Logo black"></a>
            </div>
            <!--Right Col-->
            <div class="nav-outer">
              <!-- Main Menu -->
              <nav class="main-menu">
                <div class="navbar-collapse show collapse clearfix">
                  <ul class="navigation clearfix">
                    <!--Keep This Empty / Menu will come through Javascript-->
                  </ul>
                </div>
              </nav><!-- Main Menu End-->
              <!--Mobile Navigation Toggler-->
              <div class="mobile-nav-toggler"><span class="icon lnr-icon-bars"></span></div>
            </div>
            <% if (typeof itemType !== 'undefined' && itemType === 'stay') { %>
            <!-- Book Now Button for Stay Pages -->
            <div class="sticky-header-book-btn">
              <button class="theme-btn btn-style-one" onclick="openStayBookingModal()">
                <span class="btn-title">Book Now</span>
              </button>
            </div>
            <% } %>
          </div>
        </div>
      </div><!-- End Sticky Menu -->
    </div>
	</header>
	<!--End Main Header -->
